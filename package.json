          "file": "data/Spritesheet.bin"
        },
        {
          "type": "raw",
          "name": "DATA_LOCATIONS",
          "file": "data/Locations.bin"
        },
        {
          "type": "font",
          "name": "FONT_DIGITAL_22",
          "file": "fonts/pebblemon-time-digital.ttf",
          "characterRegex": "[0-9]"
        },
        {
          "type": "font",
          "name": "FONT_OUTLINES_23",
