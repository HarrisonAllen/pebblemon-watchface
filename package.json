{
  "name": "pebblemon-watchface",
  "author": "Harrison Allen",
  "version": "2.0.0",
  "keywords": [
    "pebble-app"
  ],
  "private": true,
  "dependencies": {
    "pebble-clay": "^1.0.4"
  },
  "capabilities": [
    "configurable"
  ],
  "pebble": {
    "displayName": "Pebblemon Time",
